package main

@concept{*
    T has [ func + T -> T ],
    ! T implements IProgram,
    S identifier
    // tests could be generated too
*}
object Box<T, S>
    func init: T value { 
       self.value = value 
    }
    func sum: T other -> T = 
        value + other;
        
    // parameter used as method keyword
    func S -> String = #S;
    
    @property T value
end
