package grammar

object GMTest<T>
    @grammarMethod{*
        (add: (T)* ) 
    *}
    @grammarMethod{*
        (each: T)+
    *}
    func addAll: Array<T> args {
        all addAll: args    
    }
    
    func init { all = Array<T>(); }
    
    override
    func asString -> String = all asString;
    
    func getAll -> Array<T> = all;
    
    let Array<T> all
   
end
