package grammar

open
object MyOddArray
    @grammarMethod{*
        ( (add: Int)+ (add: Int)* )
    *}
    func addAll: Tuple<Array<Int>, Array<Int>> t {  
        all addAll: t f1;
        assert t f2 size == 0;
        all addAll: t f2;
    }
    
    func init { all = Array<Int>(); }
    
    override
    func asString -> String = all asString;
    
    @property let Array<Int> all
       
end
