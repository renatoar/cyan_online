package main


object Test

    func init: String name, Test|Nil next {
        self.name = name;
        self.next = next;
    }
    
    func init {
        self.next = Nil
    }
    @annot("xml")
    var String name = "aTest";
    
    @annot("xml")
    var Test|Nil next;
    
    func getName -> String = name;
    func getNext -> Test|Nil = next;

    @annot("callThis")
    func add10: Int n -> Int = n + 10;
    
    @annot("callThis")
    func twice: Int n -> Int = 2*n;
    
    @annot("callThis")
    func cube: Int n -> Int = n*n*n;
    
    @annot("callThis")
    func mult: Int n, Int p -> Int = n*p;

    
end
