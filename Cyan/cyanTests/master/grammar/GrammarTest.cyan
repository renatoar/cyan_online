package grammar

object GrammarTest
    func run {
        let car = Car();
        var ok = car do:      on:        move: 100
            left:    move: 50   right:
            right:   move: 200  off:  == 
            "car on car move(100) car left car move(50) car right car right car move(200) car off ";
            
        assert ok;
    
    }
   
end
