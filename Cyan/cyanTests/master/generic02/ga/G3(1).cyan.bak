package generic02.ga

object G3<T>
    fun init { 
        _value = G1<T>;
    }
    fun init: G1<T> first {
        _value = first;
    }
    fun nameGP -> String {
        return T prototypeName
    }
    
    fun name -> String = "generic02.ga." + prototypeName;
    fun value -> G1<T> = _value;
    fun nameT -> String = ( @paramToString(T) );
    
    fun check: String tname {
        const tproto = T prototypeName;
        assert tproto == tname;
    }
    
    G1<T> _value
end

