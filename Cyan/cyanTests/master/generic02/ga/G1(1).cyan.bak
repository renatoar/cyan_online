package generic02.ga

object G1<T>
    fun init {
        _value = G2<T> new;
    }
    fun init: G2<T> _value {
        self._value = _value
    }
    fun nameGP -> String {
        return T prototypeName
    }
    fun name -> String = "generic02.ga." + prototypeName;
    fun value -> G2<T> = _value;
    
    fun nameT -> String = ( @paramToString(T) );
    
    fun check: String tname, T t,  String protoName {
        var T t0 = t;
        const T t1 = t;
        const tproto = T prototypeName;
        assert  tproto == tname && self prototypeName == protoName &&
           t0 prototypeName == tname && t1 prototypeName == tname;
    }
    
    G2<T> _value
    
end
