package main

object Person
    func init: String name,
               Int age {
        self.name = name;
        self.age = age
    }
    func name: String name
         age: Int age {
        self.name = name;
        self.age = age
    }
    func getName -> String = name;
    func setName: String name {
        self.name = name
    }
    func getAge -> Int = age;
    func setAge: Int age {
        self.age = age
    }
    var String name
    var Int age
end
