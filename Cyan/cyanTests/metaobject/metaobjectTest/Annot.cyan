package metaobjectTest


@annot("correct this")
@annot( [ 2, 3, 5, 7 ] )
object Annot

    func run {
        var Int count = 0;
    
        for Any any in self annotList {
            if any == "correct this" {
                ++count
            }
            else if any == [ 2, 3, 5, 7 ] {
                ++count
            }
        }
        assert count == 2;
    }
    
end
